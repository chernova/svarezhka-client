// Line Awesome

// PT Sans
@font-face 
    font-family 'PT Sans'
    src local('PT Sans'), local('PTSans-Regular'), url('/static/fonts/PTSans/ptsans.woff2') format('woff2'), url('/static/fonts/PTSans/ptsans.woff') format('woff'), url('/static/fonts/PTSans/ptsans.ttf') format('truetype')
    font-weight 400
    font-style normal

@font-face 
    font-family 'PT Sans'
    src local('PT Sans Italic'), local('PTSans-Italic'), url('/static/fonts/PTSans/ptsansitalic.woff2') format('woff2'), url('/static/fonts/PTSans/ptsansitalic.woff') format('woff'), url('/static/fonts/PTSans/ptsansitalic.ttf') format('truetype')
    font-weight 400
    font-style italic

@font-face 
    font-family 'PT Sans'
    src local('PT Sans Bold'), local('PTSans-Bold'), url('/static/fonts/PTSans/ptsansbold.woff2') format('woff2'), url('/static/fonts/PTSans/ptsansbold.woff') format('woff'), url('/static/fonts/PTSans/ptsansbold.ttf') format('truetype')
    font-weight 700
    font-style normal

@font-face 
    font-family 'PT Sans'
    src local('PT Sans Bold Italic'), local('PTSans-BoldItalic'), url('/static/fonts/PTSans/ptsansbolditalic.woff2') format('woff2'), url('/static/fonts/PTSans/ptsansbolditalic.woff') format('woff'), url('/static/fonts/PTSans/ptsansbolditalic.ttf') format('truetype')
    font-weight 700
    font-style italic

